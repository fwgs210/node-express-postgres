{
    "builds": [
        { "src": "index.js", "use": "@now/node" },
        { "src": "node_modules/swagger-ui-dist/index.js", "use": "@now/node" }
    ],
    "routes": [
        { "src": "/documentation/(.*)", "dest": "/node_modules/swagger-ui-dist/index.js" },
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}